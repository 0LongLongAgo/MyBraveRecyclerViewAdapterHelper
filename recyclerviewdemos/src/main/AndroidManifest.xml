<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="signin.company.com.recyclerviewdemo">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".simpleuse.LinearLayoutManagerRecyclerview" />
        <activity android:name=".simpleuse.LinearLayoutManagerTimeLineRecyclerview" />
        <activity android:name=".simpleuse.SimpleUseActivity" />
        <activity android:name=".simpleuse.GridLayoutManagerRecyclerview" />
        <activity android:name=".simpleuse.StaggeredLayoutManagerRecyclerview" />
        <activity android:name=".fragment.MyFragmentActivity" />
        <activity android:name=".baserecyclerviewadapterhelper.pulltorefresh.PullToRefreshAndPushDownloadActivity"></activity>
    </application>

</manifest>